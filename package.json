{
  "name": "eleanor",
  "version": "0.0.7",
  "description": "Eleanor: A pzpack gui",
  "main": "main.cjs",
  "files": [
    "main.cjs",
    "dist",
    "assets",
    "index.html"
  ],
  "scripts": {
    "start": "electron . --dev",
    "check": "tsc --noEmit",
    "lint": "eslint --cache --ext .ts --format=pretty ./src",
    "lint:prettier": "prettier -c ./src/**/*",
    "prettier": "prettier --write ./src/**/*",
    "build": "npm run build:prod && npm run build:css:prod",
    "pack": "electron-builder -p never --config electron-builder.config.cjs",
    "build:dev": "node ./esbuild.cjs",
    "build:prod": "node ./esbuild.cjs --prod",
    "build:css": "tailwindcss -i ./src/style/index.css -o ./build/style.css",
    "build:css:prod": "tailwindcss -i ./src/style/index.css -o ./build/style.css --minify",
    "watch:dev": "node ./esbuild.cjs --watch",
    "watch:css": "tailwindcss -i ./src/style/index.css -o ./build/style.css --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/natural-compare-lite": "^1.4.0",
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.2",
    "electron": "^17.3.0",
    "electron-builder": "^22.14.13",
    "esbuild": "^0.14.24",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dashjs": "^4.3.0",
    "i18next": "^21.6.13",
    "i18next-resources-to-backend": "^1.0.0",
    "natural-compare-lite": "^1.4.0",
    "parse-xsd-duration": "^0.5.0",
    "pzpack": "../pzpack.js",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.5",
    "xml2js": "^0.4.23"
  }
}
